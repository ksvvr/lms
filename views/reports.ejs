<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Course Report</title>
    <script src="https://cdn.tailwindcss.com"></script>
</head>

<body class="text-center">
    <br>
    <a href="/signout" class="bg-lime-300">Signout</a>
    <a href="/dashboard" class="bg-lime-300">Home</a>
    <h2 class="font-bold text-3xl mt-2">Your Courses Report</h2>
    <br>
    <%- include('notifications.ejs') %>
    <br>
    <% courseReports.forEach(courseReport=> { %>
        <div>
            <h3 class="font-bold mt-4">
                <%= courseReport.courseName %>
            </h3>
            <p>Enrollment Count: <%= courseReport.enrollmentCount %>
            </p>
        </div>
    <% }); %>

</body>

</html>